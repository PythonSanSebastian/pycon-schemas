{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://pycon.org/presentation.json",
  "title": "Presentation",
  "description": "A schema for a public view of a presentation",
  "type": "object",
  "required": [
    "title"
  ],
  "properties": {
    "title": {
      "description": "Title of the presentation",
      "type": "string",
      "examples": [
        "Introduction to jsonschema"
      ]
    }
  }
}
